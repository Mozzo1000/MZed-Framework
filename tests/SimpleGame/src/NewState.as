package {	import mzed.framework.*;	import mzed.framework.render.MZ_Object;		/**	 * ...	 * @author Mozzo	 */	public class NewState extends MZ_State {				[Embed(source = "../res/Icon.png")] public static var coin:Class;		public var spr1:MZ_Object;		public var spr2:MZ_Object;		public var spr3:MZ_Object;				override public function create():void {			super.create();			trace("the new state! :O");			spr1 = new MZ_Object(0, 0, coin);			spr1.x = 400;			MZ_Base.add(spr1);			spr2 = new MZ_Object(0, 0, coin);			spr2.x = 100;			MZ_Base.add(spr2);			spr3 = new MZ_Object(100, 0, coin);			spr3.y = 400;			MZ_Base.add(spr3);		}				override public function destroy():void {			super.destroy();		}				override public function update():void {			if (MZ_Base.keys.justPressed("K")) {				MZ_Base.changeState(new State());			}		}	}}